@{
    if (string.IsNullOrEmpty(SettingsManager.Instance.Branding))
    {
        Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_LayoutSPMO.cshtml";
    }
    ViewBag.Title = "Home Page";
}
<div class="row" data-ng-show="Branding==''">
    <input type="hidden" value="homepage" id="pagename" />
    <div class="col-sm-4" data-ng-repeat="vtype in VendorTypes">
        <div class="card">
            <div class="card-block" data-ng-click="RedirectToVendorList()">
                <h4 class="card-title">{{vtype.VendorType}}</h4>
                <h6 class="card-subtitle text-muted">{{vtype.VendorType}}</h6>
            </div>
            <img src="{{vtype.TypeImagesPath}}" alt="Card image" class="img-rounded" data-ng-click="RedirectToVendorList(vtype.VendorType)">
            <div class="card-block">
                <p class="card-text" data-ng-click="RedirectToVendor()">आपला व्यवसाय वाढवण्यासाठी येथे नोंदणी करा.</p>
                <button type="button" class="btn btn-primary" data-ng-click="RedirectToVendor()">नोंदणी</button>
            </div>
        </div>
    </div>
</div>
<section id="services" data-ng-show="Branding=='SPMO'">
    <div class="container">
        <header class="section-header wow fadeInUp">
            <h3>Services</h3>
        </header>
        <div class="row">
            <div class="col-md-2" data-wow-duration="1.4s" data-ng-repeat="vtype in VendorTypes">
                <h4 class="title"><a href="@Url.Action("Vendors","Vendor")">{{vtype.VendorType}}</a></h4>
                <div class="col-md-4">
                    <img src="{{vtype.TypeImagesPath}}" alt="Card image" class="img-rounded" data-ng-click="RedirectToVendorList(vtype.VendorType)">
                </div>
                <button type="button" class="btn btn-primary btnMargin" data-ng-click="RedirectToVendor()">Register</button>
            </div>
        </div>
    </div>
</section>