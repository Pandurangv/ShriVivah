@model ShriVivah.Models.RegisterViewModel
@{
    ViewBag.Title = "Edit User";
    Layout = "~/Views/Shared/_LayoutSPMO.cshtml";
}
@Styles.Render("~/Content/AdminPanel/jquery.timepicker.css")
@Scripts.Render("~/Scripts/HelperJS/jquery.timepicker.js")
@Scripts.Render("~/Scripts/Main/jquery.date-dropdowns.min.js")
@Scripts.Render("~/bundles/EditUserProfile")
<script type="text/javascript">
    $(document).ready(function () {
        $('#TimeofBirth').timepicker({ step: '1' });
        var minAge = $("#hdnMinAge").val();
        
        bindData();
        DisableControls();
        $("#DOB").dateDropdowns({
            minAge: minAge
        });
    });
</script>

<div class="grid-3">
    <div class="container">
        @using (Html.BeginForm("", "", FormMethod.Post, new { id = "frmUserProfile" }))
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal">
                @Html.HiddenFor(m => m.UserId)

                @Html.ValidationSummary(true)
                <input type="hidden" value="@ViewBag.UserDetails" id="UserDetails" />
                <input type="hidden" id="userid" />
                <input type="hidden" id="hdnMinAge" value="@ViewBag.MinAge" />

                <input type="hidden" id="pagename" value="edituser" />
                <div class="form-group" id="contentdiv">
                    <div id="divPersonal">
                        <div class="col-md-12 divPadding">
                            <div class="headerdiv">Personal Information</div>
                            @*<div class="form-group">
                        <div class="col-md-4">

                        </div>
                    </div>*@
                            <div class="form-group">
                                <div class="form-group col-md-12">
                                    <label for="select" class="col-sm-2 control-label">Gender<span style="color:red;font-size:x-large">*</span></label>
